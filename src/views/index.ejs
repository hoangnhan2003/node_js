<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link rel="stylesheet" href="/css/style.css"/>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-rbsA2VBKQhggwzxH7pPCaAqO46MgnOM80zW1RWuH61DGLwZJEdK2Kadq2F9CUG65" crossorigin="anonymous">
</head>
<body>
  <div class="topnav">
    <a href="/">Home</a>
    <a href="/upload" class="active">Upload file</a>
</div>

    <h1>Hello world from ejs</h1>
    <!-- <%= dataUser  %> -->
    <table class="table table-striped">
        <% for(var i=0; i < dataUser.length; i++) { %>
            <tr>
              <td><%= dataUser[i].TeamName %></td>
              <td><%= dataUser[i].UniversityName %></td>
              <td><%= dataUser[i].ProblemId %></td>
              <td><%= dataUser[i].Time %></td>
              <td><%= dataUser[i].Result %></td>
              <td><a href="/edit-ICPC/<%= dataUser[i].TeamName %>">Edit</a></td>
              <td><a href="/delete-ICPC/<%= dataUser[i].TeamName %>">Delete</a></td>
            </tr>
         <% } %>    
    </table>
    <img src="/image/anh.jpg"/>
    <form method="post" action="/createNewICPC">
        <div class="mb-3">
            <label for="exampleFormControlInput1" class="form-label" >TeamName</label>
            <input type="text" class="form-control" id="exampleFormControlInput1" placeholder="name@example.com" name="TeamName">
          </div>
          <div class="mb-3">
            <label for="exampleFormControlTextarea1" class="form-label">UniversityName</label>
            <input type="text" class="form-control" id="exampleFormControlTextarea1" rows="3" name="UniversityName">
          </div>
          <div class="mb-3">
            <label for="exampleFormControlTextarea1" class="form-label">ProblemId</label>
            <input type="text" class="form-control" id="exampleFormControlTextarea1" rows="3" name="ProblemId">
          </div>
          <div class="mb-3">
            <label for="exampleFormControlTextarea1" class="form-label">Time</label>
            <input type="text" class="form-control" id="exampleFormControlTextarea1" rows="3" name="Time">
          </div>
          <div class="mb-3">
            <label for="exampleFormControlTextarea1" class="form-label">Result</label>
            <input type="text" class="form-control" id="exampleFormControlTextarea1" rows="3" name="Result">
          </div>
          <button type="submit">Submit</button>
    </form>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-kenU1KFdBIe4zVF0s0G1M5b4hcpxyD9F7jL+jjXkk+Q2h455rYXK/7HAuoJl+0I4" crossorigin="anonymous"></script>
</body>
</html>